{
  "singleQuote": true,
  "arrowParens": "avoid",
  "printWidth": 120,
  "tabWidth": 2,
  "semi": true,
  "trailingComma": "es5",
  "plugins": ["@shopify/prettier-plugin-liquid"],
  "liquidSingleQuote": true,
  "embeddedSingleQuote": true,
  "overrides": [
    {
      "files": "*.liquid",
      "options": {
        "parser": "liquid-html",
        "htmlWhitespaceSensitivity": "ignore"
      }
    }
  ]
}
